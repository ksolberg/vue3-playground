{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "baseUrl": "./",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    // "types": [
    //   "cypress",
      // "vite/client",
      // "vite-svg-loader"
    // ]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  // "exclude": ["dist", "node_modules"]
}
